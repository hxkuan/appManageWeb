<!DOCTYPE html><html><head>    <title>应用信息</title>    <script type="text/javascript" src="/javascripts/tools.js"></script></head><body><!-- version  --><div class="main-counters margin-bottom-40">    <div class="headline"><h2>应用信息</h2></div>    <div class="col-md-12">        <div class="row">            <table class="table text-center pgy-table table-hover ">                <thead>                <tr class="pgy-warning" style="background-color: #f7f7f7;border-top:1px solid #ddd">                    <th class="text-center" width="20%">名称</th>                    <th class="text-center" width="20%">当前版本</th>                    <th class="text-center" width="20%">最后更新日期</th>                    <th class="text-center" width="40%">操作</th>                </tr>                </thead>                <tbody id="version_result">                <% data.forEach(function (app){ %>                <tr>                <th class="text-center" width="15%"><%= app.appCName %>(<%= app.appEName %>)</th>                <th class="text-center" width="15%"><%= app.versionName %>(<%= app.versionCode %>)</th>                <th class="text-center" width="15%"><%= app.uploadTime  %></th>                <th class="text-center" width="55%"><a href="javascript:downloadFile('<%= apkDir+app.url%>')">下载</a> <a href="/app/detail?id=<%= app.id %>">查看</a> </th>                </tr>                <% }) %>                </tbody>            </table>        </div>    </div></div></div></body></html>